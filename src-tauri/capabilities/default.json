{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-kill",
    "shell:allow-open",
    "core:window:default",
    "core:window:allow-close",
    "core:window:allow-set-title",
    "core:window:allow-request-user-attention",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-close",
    "sql:allow-select"
  ]
}