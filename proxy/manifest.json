{
    "manifest_version": 3,
    "name": "Proxy Setter",
    "version": "1.0",
    "description": "简单的代理设置工具",
    "key": "",
    "permissions": [
        "identity",
        "storage",
        "proxy",
        "webRequest",
        "privacy",
        "webRequestAuthProvider",
        "cookies"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    }
}